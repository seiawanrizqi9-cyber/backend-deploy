model Order {
  id          Int         @id @default(autoincrement())
  user_id     Int         
  total       Decimal     @db.Decimal(10, 2) @default(0)
  user        User[]
  orderItems  OrderItem[]
  createdAt   DateTime    @default(now())
  updatedAt   DateTime    @updatedAt
  deletedAt   DateTime?

  @@map("orders")
}